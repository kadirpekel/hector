{
  "annotations": {
    "list": []
  },
  "description": "Comprehensive Hector HTTP and gRPC Performance Dashboard",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "description": "P99, P95, and P50 latency for HTTP requests",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Duration (seconds)",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max"]},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(hector_http_request_duration_seconds_bucket[5m])) by (le, method, path))",
          "legendFormat": "P99 - {{method}} {{path}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(hector_http_request_duration_seconds_bucket[5m])) by (le, method, path))",
          "legendFormat": "P95 - {{method}} {{path}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(hector_http_request_duration_seconds_bucket[5m])) by (le, method, path))",
          "legendFormat": "P50 - {{method}} {{path}}",
          "refId": "C"
        }
      ],
      "title": "HTTP Request Latency",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "HTTP requests per second by status code",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Requests/sec",
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "last"]},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(rate(hector_http_requests_total{status_code=~\"2..\"}[5m]))",
          "legendFormat": "2xx Success",
          "refId": "A"
        },
        {
          "expr": "sum(rate(hector_http_requests_total{status_code=~\"3..\"}[5m]))",
          "legendFormat": "3xx Redirect",
          "refId": "B"
        },
        {
          "expr": "sum(rate(hector_http_requests_total{status_code=~\"4..\"}[5m]))",
          "legendFormat": "4xx Client Error",
          "refId": "C"
        },
        {
          "expr": "sum(rate(hector_http_requests_total{status_code=~\"5..\"}[5m]))",
          "legendFormat": "5xx Server Error",
          "refId": "D"
        }
      ],
      "title": "HTTP Request Rate by Status Code",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "Distribution of HTTP status codes",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Requests",
            "drawStyle": "bars",
            "fillOpacity": 80,
            "lineWidth": 0
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["last"], "sortBy": "Last", "sortDesc": true},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(hector_http_requests_total) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Status Code Distribution",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "HTTP error rate over time",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.01},
              {"color": "red", "value": 0.05}
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "options": {
        "orientation": "auto",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "sum(rate(hector_http_requests_total{status_code=~\"[45]..\"}[5m])) / sum(rate(hector_http_requests_total[5m]))",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "title": "HTTP Error Rate",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
