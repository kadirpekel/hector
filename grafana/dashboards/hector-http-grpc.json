          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(hector_http_response_size_bytes_bucket[5m])) by (le, path))",
          "legendFormat": "P50 - {{path}}",
          "refId": "C"
        }
      ],
      "title": "HTTP Response Size",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "Request rate by HTTP endpoint",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Requests/sec",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "id": 6,
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "last"], "sortBy": "Last", "sortDesc": true},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(rate(hector_http_requests_total[5m])) by (method, path)",
          "legendFormat": "{{method}} {{path}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Requests by Endpoint",
      "type": "timeseries"
    },
    {
